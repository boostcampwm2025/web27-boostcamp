{
  "name": "web27-boostcamp",
  "version": "1.0.0",
  "description": "RTB 광고 플랫폼",
  "private": true,
  "scripts": {
    "dev": "cd sdk && npm run build && cd .. && concurrently \"cd backend && npm run start:dev\" \"cd frontend && npm run dev\"",
    "build": "cd frontend && npm run build && cd ../backend && npm run build",
    "build:sdk": "cd sdk && npm run build",
    "lint": "cd backend && npm run lint && cd ../frontend && npm run lint && cd ../sdk && npm run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostcampwm2025/web27-BoostAD.git"
  },
  "author": "Team Web27",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boostcampwm2025/web27-boostcamp/issues"
  },
  "homepage": "https://github.com/boostcampwm2025/web27-boostcamp#readme",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.4.2"
  }
}
